{
  "name": "offshop",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "13.6.0",
    "npm": "6.14.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.6",
    "D": "^1.0.0",
    "auth0-js": "^9.13.1",
    "aws-amplify": "^2.3.0",
    "aws-amplify-react": "^3.1.9",
    "axios": "^0.19.2",
    "bfj": "^7.0.2",
    "chalk": "^3.0.0",
    "classnames": "^2.2.6",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "fs-extra": "^8.1.0",
    "identity-obj-proxy": "3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "normalizr": "^3.6.0",
    "postcss-flexbugs-fixes": "4.2.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.11.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.2",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.20",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-form": "^8.3.2",
    "redux-thunk": "^2.3.0",
    "resolve": "^1.15.1",
    "socket.io-client": "^2.3.0",
    "typeface-roboto": "0.0.75",
    "web-push": "^3.4.3",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "dev:hot": "npm start -- --hot",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "format": "pretty-quick",
    "heroku-prebuild": "npm install --production=false"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "describe": "readonly",
      "beforeEach": "readonly",
      "fakeLocalStorage": "readonly",
      "renderWithAuth": "readonly",
      "renderWithStore": "readonly",
      "renderWithRouter": "readonly",
      "google": "readonly",
      "socket": "readonly"
    },
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/register": "^7.9.0",
    "@svgr/webpack": "^5.3.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^9.5.0",
    "axios-mock-adapter": "^1.18.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "coveralls": "^3.0.11",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^5.1.0",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "history": "^4.10.1",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.2.7",
    "jest-dom": "^4.0.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^25.2.6",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-dev-utils": "^10.1.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "sass-loader": "^8.0.2",
    "socket.io-mock": "^1.2.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "./testUtils.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
